{
    "name": "ndf2json",
    "description": "Convert Eugen Systems .ndf syntax to .json",
    "version": "1.0.1",
    "author": "Dmytro Vasylkivskyi",
    "repository": "https://github.com/drizzer14/ndf2json",
    "type": "module",
    "sideEffects": false,
    "module": "index.js",
    "types": "index.d.ts",
    "scripts": {
        "start": "parcel debugger/index.html --port 7802 --no-cache",
        "prebuild": "rimraf lib",
        "build": "tsc --build tsconfig.build.json",
        "postbuild": "node scripts/package.js && node scripts/tsconfig.js"
    },
    "dependencies": {
        "ts-pattern": "5.3.1"
    },
    "devDependencies": {
        "@types/node": "22.7.5",
        "@types/react": "18.3.7",
        "@types/react-dom": "18.3.0",
        "chalk": "5.3.0",
        "fnts": "3.0.1",
        "parcel": "2.12.0",
        "prettier": "3.3.3",
        "process": "0.11.10",
        "react": "18.3.1",
        "react-dom": "18.3.1",
        "react-json-view": "1.21.3",
        "rimraf": "6.0.1",
        "styled-components": "6.1.13",
        "typescript": "5.6.2"
    }
}
